{
  "name": "vscode-documents-scripting",
  "displayName": "VS Code extension for documents scripting",
  "description": "",
  "version": "0.0.4",
  "publisher": "otris-software",
  "author": {
      "name": "otris software",
      "email": "heller@otris.de"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.downloadAllScripts",
    "onCommand:extension.downloadScript",
    "onCommand:extension.uploadAllScripts",
    "onCommand:extension.uploadScript",
    "onCommand:extension.runScript",
    "onCommand:extension.saveConfiguration",
    "onCommand:extension.loadConfiguration"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.downloadAllScripts",
        "title": "Documents: Download Scripts To Folder"
      },
      {
        "command": "extension.downloadScript",
        "title": "Documents: Download Script"
      },
      {
        "command": "extension.uploadAllScripts",
        "title": "Documents: Upload Scripts From Folder"
      },
      {
        "command": "extension.uploadScript",
        "title": "Documents: Upload Current Script"
      },
      {
        "command": "extension.runScript",
        "title": "Documents: Run Current Script"
      },
      {
        "command": "extension.saveConfiguration",
        "title": "Documents Configuration: Save"
      },
      {
        "command": "extension.loadConfiguration",
        "title": "Documents Configuration: Load"
      },
      {
        "command": "extension.clearConfiguration",
        "title": "Documents Configuration: Clear"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.uploadAllScripts"
        },
        {
          "command": "extension.downloadAllScripts"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "typescript": "^2.2.1",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "net": "^1.0.2",
    "node-sds": "git+https://git@github.com/otris/node-sds.git",
    "path": "^0.12.7",
    "reduce-for-promises": "^1.2.1",
    "typescript-compiler": "^1.4.1-2",
    "vsce": "^1.18.0"
  }
}
