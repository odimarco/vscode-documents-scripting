{
  "name": "vscode-documents-scripting",
  "displayName": "VS Code extension for portal scripting",
  "description": "",
  "version": "0.0.1",
  "publisher": "heller",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.downloadAllScripts",
    "onCommand:extension.uploadScript",
    "onCommand:extension.runScript",
    "onCommand:extension.saveConfiguration"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.downloadAllScripts",
        "title": "Documents scripting: download all"
      },
      {
        "command": "extension.uploadScript",
        "title": "Documents scripting: upload current"
      },
      {
        "command": "extension.runScript",
        "title": "Documents scripting: run current"
      },
      {
        "command": "extension.saveConfiguration",
        "title": "Documents configuration: save"
      },
      {
        "command": "extension.loadConfiguration",
        "title": "Documents configuration: load"
      },
      {
        "command": "extension.clearConfiguration",
        "title": "Documents configuration: clear"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "package": "vsce package",
    "installextension": "code --install-extension vscode-documents-scripting-0.0.1.vsix",
    "//": "reference version in script installextenstion?"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "typescript": "^2.2.1",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "net": "^1.0.2",
    "node-sds": "git+https://git@github.com/otris/node-sds.git",
    "path": "^0.12.7",
    "reduce-for-promises": "^1.2.1",
    "typescript-compiler": "^1.4.1-2",
    "vsce": "^1.18.0"
  }
}
